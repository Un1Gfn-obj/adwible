<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.0"/>
  <object class="AdwApplicationWindow" id="tf2fhx">
    <style><class name="flat"/></style>
    <property name="content">

      <object class="AdwCarousel" id="carousel">
        <property name="vexpand">True</property>
        <property name="hexpand">True</property>
        <property name="allow-long-swipes">TRUE</property>
        <property name="allow-mouse-drag">TRUE</property>
        <property name="allow-scroll-wheel">TRUE</property>
        <property name="orientation">GTK_ORIENTATION_HORIZONTAL</property>

          <child>
            <object class="GtkScrolledWindow" id="qbtw37">
              <property name="vexpand">TRUE</property>
              <property name="hexpand">TRUE</property>
              <property name="kinetic-scrolling">TRUE</property>
              <property name="vscrollbar-policy">GTK_POLICY_ALWAYS</property>
              <property name="window-placement">GTK_CORNER_BOTTOM_RIGHT</property>
              <child>
                <object class="GtkBox" id="qgnxl8">
                  <property name="orientation">vertical</property>
                  <property name="margin-top">12</property>
                  <property name="margin-start">12</property>
                  <property name="margin-end">12</property>
                  <property name="margin-bottom">32</property>
                  <property name="spacing">12</property>
                    <!-- add child bookgroups here -->
                </object> <!-- END GtkBox -->
              </child>
            </object> <!-- END GtkScrolledWindow OT -->
          </child>

          <child>
            <object class="GtkScrolledWindow">
              <property name="vexpand">TRUE</property>
              <property name="hexpand">TRUE</property>
              <child>
                <object class="GtkBox">
                  <property name="orientation">vertical</property>
                  <property name="margin-top">12</property>
                  <property name="margin-start">12</property>
                  <property name="margin-end">12</property>
                  <property name="spacing">12</property>
                  <child>
                    <object class="AdwPreferencesGroup">
                      <property name="title">Canonical gospels | Evangelia canonica</property>
                      <child>
                        <object class="AdwExpanderRow">
                          <property name="title">Matthew</property>
                          <property name="subtitle">Evangelium secundum Matthaeum</property>
                          <property name="expanded">TRUE</property>
                          <child>
                            <object class="GtkFlowBox">
                              <property name="max-children-per-line">255</property>
                              <child><object class="GtkToggleButton"><property name="halign">GTK_ALIGN_START</property><property name="label">1</property></object></child>
                            </object>
                          </child>
                        </object> <!-- END AdwExpanderRow "Matthew" -->
                      </child>
                      <child>
                        <object class="AdwExpanderRow">
                          <property name="title">Mark</property>
                          <property name="subtitle">Evangelium secundum Marcum</property>
                          <property name="expanded">TRUE</property>
                          <child>
                            <object class="GtkFlowBox">
                              <property name="max-children-per-line">255</property>
                              <child><object class="GtkToggleButton"><property name="halign">GTK_ALIGN_START</property><property name="label">1</property></object></child>
                            </object>
                          </child>
                        </object> <!-- END AdwExpanderRow "Mark" -->
                      </child>
                      <child>
                        <object class="AdwExpanderRow">
                          <property name="title">et al.</property>
                          <property name="subtitle">...</property>
                        </object> <!-- END AdwExpanderRow "et al." -->
                      </child>
                    </object> <!-- END AdwPreferencesGroup "gospels" -->
                  </child>
                </object> <!-- END GtkBox -->
              </child>
            </object> <!-- END GtkScrolledWindow NT -->
          </child>

      </object> <!-- END AdwCarousel -->

    </property> <!-- END AdwApplicationWindow.content -->
  </object> <!-- END AdwApplicationWindow -->
</interface>
